# Frontend environment variables
# Copy this to .env (or .env.local) and fill in your deployed addresses

# Default chain id (11155111 = Sepolia)
VITE_DEFAULT_CHAIN_ID=11155111

# Optional: RPC URL for the default chain (if omitted, wallet provider will be used)
# e.g., https://rpc.ankr.com/eth_sepolia
VITE_RPC_URL=

# Contract addresses (fill after deployment)
VITE_USER_REGISTRY_ADDRESS=
VITE_ORG_REGISTRY_ADDRESS=
VITE_AUDIT_LOGGER_ADDRESS=

